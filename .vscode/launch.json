    "version": "0.2.0",
    "configurations": [        
        {
            "name": "get_cli create project",
            "request": "launch",
            "program": "${workspaceFolder}/bin/get.dart",
            "args": [
                "create",
                "project"
            ],
            "cwd": "${workspaceFolder}",
            "type": "dart",     
            "internalConsoleOptions": "openOnSessionStart",
            "console": "terminal"
        },
        {
            "name": "get_cli create page",
            "request": "launch",
            "program": "${workspaceFolder}/bin/get.dart",
            "args": [
                "create",
                "page:test1"
            ],
            "cwd": "${workspaceFolder}/example_debug",
            "type": "dart",            
            "console": "terminal"
        },
        {
            "name": "get_cli init",
            "request": "launch",
            "program": "${workspaceFolder}/bin/get.dart",
            "args": [
                "init"
            ],
            "cwd": "${workspaceFolder}/example_debug",
            "type": "dart",            
            "console": "terminal"
        },
    ]
}